<%= form_for([@workout, @workout.exercises.build]) do |f|  %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :sets %><br>
    <%= f.text_field :sets %>
  </p>
  <p>
    <%= f.label :reps %><br>
    <%= f.text_field :reps %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
